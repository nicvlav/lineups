{
  "name": "lineups",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://lineups.nicolasvlavianos.workers.dev",
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite",
    "build": "bun run typecheck && bun --bun vite build",
    "build:node": "npm run typecheck:node && npx vite build",
    "typecheck": "bun tsc -b --noEmit",
    "typecheck:node": "npx tsc -b --noEmit",
    "lint": "bun eslint .",
    "lint:fix": "bun eslint . --fix",
    "preview": "bun --bun vite preview",
    "preview:worker": "bunx wrangler preview",
    "deploy:worker": "bunx wrangler build && bunx wrangler deploy",
    "clean": "rimraf dist node_modules/.vite tsconfig.tsbuildinfo",
    "install:clean": "bun run clean && bun install"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.4.0",
    "@cloudflare/workers-types": "^4.20250412.0",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-context-menu": "^2.2.7",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-hover-card": "^1.1.11",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-radio-group": "1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "^4.1.3",
    "antd": "^5.24.6",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.6.5",
    "idb": "^8.0.2",
    "lucide-react": "^0.479.0",
    "lz-string": "^1.5.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-device-detect": "^2.2.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "sonner": "2.0.7",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "wrangler": "^4.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "rimraf": "^6.0.1",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.1.34"
}