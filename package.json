{
  "name": "lineups",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://lineups.nicolasvlavianos.workers.dev",
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite",
    "build": "bun run typecheck && bun --bun vite build",
    "build:node": "npm run typecheck:node && npx vite build",
    "typecheck": "bun tsc -b --noEmit",
    "typecheck:node": "npx tsc -b --noEmit",
    "lint": "bunx @biomejs/biome check .",
    "lint:fix": "bunx @biomejs/biome check --write .",
    "format": "bunx @biomejs/biome format --write .",
    "format:check": "bunx @biomejs/biome format .",
    "preview": "bun --bun vite preview",
    "preview:worker": "bunx wrangler preview",
    "deploy:worker": "bunx wrangler build && bunx wrangler deploy",
    "clean": "rimraf dist node_modules/.vite tsconfig.tsbuildinfo",
    "install:clean": "bun run clean && bun install"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.4.0",
    "@cloudflare/workers-types": "4.20251014.0",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-radio-group": "1.3.7",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tooltip": "1.2.8",
    "@supabase/supabase-js": "2.76.1",
    "@tailwindcss/vite": "4.1.15",
    "@tanstack/react-query": "5.90.11",
    "antd": "5.27.6",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "framer-motion": "12.23.24",
    "idb": "8.0.3",
    "lucide-react": "0.479.0",
    "lz-string": "1.5.0",
    "react": "19.2.0",
    "react-device-detect": "2.2.3",
    "react-dom": "19.2.0",
    "react-router-dom": "7.9.4",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.15",
    "tailwindcss-animate": "1.0.7",
    "uuid": "11.1.0",
    "wrangler": "4.44.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@tanstack/react-query-devtools": "5.91.1",
    "@types/node": "22.18.12",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "4.7.0",
    "rimraf": "6.0.1",
    "typescript": "5.9.2",
    "vite": "6.4.1"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.1.34"
}